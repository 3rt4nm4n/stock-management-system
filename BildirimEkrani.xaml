<Window x:Class="DYSis.BildirimEkrani"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DYSis"
        mc:Ignorable="d"
        Title="BildirimEkrani" Height="500" Width="308" ResizeMode="NoResize" Icon="bildirim_0.png">
    <Grid>
        <TextBox x:Name="bildirimTxt" IsReadOnly="True" HorizontalAlignment="Left" Height="193" Margin="10,39,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="274" BorderBrush="{x:Null}" />
        <TextBox x:Name="bildirimTxt_Copy" IsReadOnly="True" HorizontalAlignment="Left" Height="190" Margin="10,270,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="274" BorderBrush="{x:Null}" />
        <Label Content="Şu Ürünler Bitti: " FontWeight="Bold" HorizontalAlignment="Left" Margin="10,244,0,0" VerticalAlignment="Top"/>
        <Label Content="Şu Ürünler Azaldı: " FontWeight="Bold" HorizontalAlignment="Left" Margin="10,13,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
